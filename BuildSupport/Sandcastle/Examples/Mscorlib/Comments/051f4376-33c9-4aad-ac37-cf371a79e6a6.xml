                <entry>
                  <para>Condition </para>
                </entry>
              </row>
            </tableHeader>
            <row>
              <entry>
                <para>Less than zero </para>
              </entry>
              <entry>
                <para>This instance is <languageKeyword>false</languageKeyword> and <parameterReference>value</parameterReference> is <languageKeyword>true</languageKeyword>. </para>
              </entry>
            </row>
            <row>
              <entry>
                <para>Zero </para>
              </entry>
              <entry>
                <para>This instance and <parameterReference>value</parameterReference> are equal (either both are <languageKeyword>true</languageKeyword> or both are <languageKeyword>false</languageKeyword>). </para>
              </entry>
            </row>
            <row>
              <entry>
                <para>Greater than zero </para>
              </entry>
              <entry>
                <para>This instance is <languageKeyword>true</languageKeyword> and <parameterReference>value</parameterReference> is <languageKeyword>false</languageKeyword>. </para>
              </entry>
            </row>
          </table>
