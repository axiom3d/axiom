<doc xmlns:dev="http://ddue.schemas.microsoft.com/authoring/2003/5" xmlns:xlink="http://www.w3.org/1999/xlink" fileAssetGuid="78388d66-4080-40b0-a696-87873577e048" revisionNumber="8">
  <assembly>
    <name>mscorlib</name>
