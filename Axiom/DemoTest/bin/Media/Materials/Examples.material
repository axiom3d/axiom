// Transparent knots
Transparency/Knot
{
	ambient 0.2 0.2 0.2
	scene_blend add
	depth_write off
	{
		texture Water01.jpg
		scroll_anim 0.25 0
	}
	{
		texture Water01.jpg
		wave_xform scroll_y sine 0 0.1 0 0.5
		colour_op modulate
	}

}

