<?xml version="1.0" encoding="utf-8" ?>
<Prebuild xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.6.xsd" version="1.6">
	<Solution name="Axiom" path=".">
		<Configuration name="Debug">
			<Options>
			<IncrementalBuild>true</IncrementalBuild>
			<CompilerDefines>DEBUG;TRACE</CompilerDefines>
			<OptimizeCode>false</OptimizeCode>
			<AllowUnsafe>true</AllowUnsafe>
			<DebugInformation>true</DebugInformation>
			<WarningLevel>0</WarningLevel>
			</Options>
		</Configuration>
		<Configuration name="Release">
			<Options>
			<CompilerDefines>TRACE</CompilerDefines>
			<OptimizeCode>true</OptimizeCode>
			<AllowUnsafe>true</AllowUnsafe>
			<DebugInformation>false</DebugInformation>
			<WarningLevel>0</WarningLevel>
			</Options>
		</Configuration>
		
		<Files>
			<Match pattern="*.build" path="." recurse="true" />
		</Files>
		
		<Project name="Axiom.Demos" path="Demos/Demos" language="C#" type="Library">
			<Configuration name="Debug">
				<Options>
					<AllowUnsafe>true</AllowUnsafe>
					<OutputPath>../../../build/debug/Axiom_Demos</OutputPath>
				</Options>
			</Configuration>
			<Configuration name="Release">
				<Options>
					<AllowUnsafe>true</AllowUnsafe>
					<OutputPath>../../../build/release/Axiom_Demos</OutputPath>
				</Options>
			</Configuration>
				
			<ReferencePath>..\..\..\..\Dependencies\Managed\DotNet</ReferencePath>

			<Reference name="System" />
			<Reference name="System.Data" />
			<Reference name="System.Drawing" />
			<Reference name="System.Xml" />
			<Reference name="Axiom" localCopy="true"/>
			
			<Files>
				<Match pattern="*.cs" path="."/>
			</Files>
		</Project>		

		<Project name="Axiom.Demos.Browser.CommandLine" path="Demos/Browser/CommandLine" language="C#" type="Exe">
